extends layout

block content
  h1= title
  p Welcome to #{msg}
    div
    table.table.table-hover
        thead
        tr
            th Work Orders Number
            th Product Type
            th Output/Hour
            th Start Date
        tbody
        each workOrder in workOrders
            tr
            td=workOrder.number
            td=workOrder.product_type
            td=workOrder.output
            td=workOrder.startDate
    a(href="new_workOrders")
   button(type="button") New Work Order
